{
  "forwardPorts": [
    3000
  ],
  "portsAttributes": {
    "3000": {
      "label": "Development Server",
      "onAutoForward": "openPreview"
    }
  },
  "updateContentCommand": "pnpm install",
  "postAttachCommand": "pnpm run dev",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "src/index.ts"
      ]
    },
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  }
}